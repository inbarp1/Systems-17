#include <stdio.h>
//Problem 1
//should return the sum of all
//the multiples of 3 or 5 below 1000
int m3and5(){
  int answer = 0;
  int current =0;
  while (current < 1000){
    if(current%3 ==0){
      answer+=current;
    }
    else{
      if(current%5==0){
        answer+=current;
      }
    }
    current++;
  }
  return answer;
}
//Problem 5
//should return the smallest positive number
//that is evenly divisible by all of the
//numbers from 1 to 20
int smallestMultiple(){
  int current =0;
  while(isDivisible(current)==0){
    current++;
  }
  return current;
}
int isDivisible(int c){
  int check =1;
  while(check < 21){
    if (c%check!=0){
      return 0;
    }
    c++;
  }
  return 1;
}

