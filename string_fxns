#include <stdio.h>
#include <string.h>

int main ()
{
   char str1[]="abcdef"'
   char str2[]="ABCDEF";
   int ans;

   ans = strncmp(str1, str2, 4);

   if(ans < 0)
   {
      printf("str1 is less than str2");
   }
   else if(ans > 0) 
   {
      printf("str2 is less than str1");
   }
   else 
   {
      printf("str1 is equal to str2");
   }
   
   return(0);
}